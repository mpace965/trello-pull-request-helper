{
  "name": "Trello Pull Request Helper",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Inserts any linked Trello card inline with a GitHub pull request.",
  "homepage_url": "https://twitter.com/mpace965",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options/options.html",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Trello Pull Request Helper",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://github.com/*"
  ]
}
